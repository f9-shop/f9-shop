
<% @page_title = $pubimg.foodname1 %> 
<% @title1 = $ashop.shopname %>
<% @title2 = $pubimg.foodname1 %> 

<% @back1 = asearch_asshop_path(id:$ashop) %>
<% @city  = asearch_asshops_path %>
<% @back2 = asearch_asshop_pubimgs_path %>


<% $pubimgs.each do |pubimg| %>

  <% if $pubimg.foodimg1.present? %>
    <% @image1 = image_tag $pubimg.foodimg1_url %>
  <% else %>
    <% @image1 = image_tag "template/logo3" %>
  <% end %>

  <% if $pubimg.foodimg2.present? %>
    <% @image2 = image_tag $pubimg.foodimg2_url %>
  <% else %>
    <% @image2 = image_tag "template/logo3" %>
  <% end %>

  <% if $pubimg.foodimg3.present? %>
    <% @image3 = image_tag $pubimg.foodimg3_url %>
  <% else %>
    <% @image3 = image_tag "template/logo3" %>
  <% end %>

<% end %>

